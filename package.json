{
  "name": "auro-react-demo",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^4.0.3",
    "semantic-release": "^17.4.4"
  },
  "devDependencies": {
    "@alaskaairux/auro-button": "^6.3.4",
    "@alaskaairux/auro-checkbox": "^1.1.2",
    "@alaskaairux/auro-header": "^1.5.1",
    "@alaskaairux/auro-input": "^1.3.5",
    "@alaskaairux/auro-radio": "^1.5.0",
    "@alaskaairux/design-tokens": "^3.3.0",
    "@alaskaairux/icons": "^4.8.1",
    "@alaskaairux/webcorestylesheets": "^3.4.0",
    "focus-visible": "^5.2.0",
    "husky": "^6.0.0",
    "sass": "^1.36.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "echo \"No tests specified\" && exit 0",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "./CHANGELOG.md",
          "changelogTitle": "# Semantic Release Automated Changelog"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "./CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "browserslist": {
    "production": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 3 edge version",
      "last 2 safari major version"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 3 edge version",
      "last 2 safari major version"
    ]
  }
}
